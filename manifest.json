{
  "manifest_version": 3,
  "name": "superBM",
  "version": "0.0.1",
  "description": "Checks if the current URL matches a bookmark.",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Check Bookmark",
    "default_icon": {
      "16": "icon-default.png"
    }
  }
}